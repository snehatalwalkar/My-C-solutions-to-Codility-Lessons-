////This code is entirely my creation and gives a 100% score on Codility

   //Given, Array A of N elements: The goal is to rotate array A K times; that is, each element of A will be shifted to the right K times        

struct Results solution(int A[], int N, int K) 
{
    int i=0;
    int j=0;
    
    struct Results result;
    
    int last;   
        
    for(i=0;i<K;i++)                 //Rotates array K times
    {
    last=A[N-1];                     //Captures the last element of array
        for(j=(N-1);j>0;j--)         //Shifts each array element at a time
        {
            A[j]=A[j-1];
        }
    A[0]= last;                      //Restores the last element captured above
    }


    result.A=A;
    result.N=N;
    return result;    
}
